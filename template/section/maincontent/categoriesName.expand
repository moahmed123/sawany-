<section id="page-interiors" class="cards">

    <!-- === cards header === -->

    <header>
        <div class="row">
            <div class="col-md-offset-2 col-md-8 text-center">
                <h2 class="title">{{ fields.title.field_value }}</h2>
                <div class="text">
                    <p>{{ fields.sectiontitle.field_value }}</p>
                </div>
            </div>
        </div>
    </header>

    <div class="row row-clean">

        <!-- === item === -->
        {% for collection in collections %}
        {% set category =  getCategoryAndChilds(collection['categories']['field_value'], 500, 500) %}
        <div class="col-md-4">
            <figure>
                <figcaption style="background-image:url({{ collection.itemImage.field_value }})">
                    <img src="{{ collection.itemImage.field_value }}" alt="" />
                </figcaption>
                <a href="{{ category[loop.index -1].href }}" class="btn btn-clean mfp-open mfp-open-scrollto">{{ category[loop.index -1].name }}</a>
            </figure>
        </div>
        {% endfor %}


    </div> <!--/row--> <!--/container-->
    <!-- === button more === -->

    <div class="wrapper-more">
        <a href="products-grid.html" class="btn btn-lg">View more</a>
    </div>
</section>