						<li class="level0 nav-9 top-level">
							<a href="{{ collection.itemlink.value }}">
								<span>{{ collection.itemname.value }}</span>
							</a>
						</li>
						{% endfor %}
						{% endif %}
						{% if header_data.dropDownLink.section_state == 'enabled' %}
						<li class="level0 nav-2 top-level parent">
							<a
								href="{{ header_data.dropDownLink.fields.itemlink.value }}"><span>{{ header_data.dropDownLink.fields.itemname.value }}</span></a>
							<ul class="level0 mobile-menu mobile-dropdown"
								style="height: 0px; padding-top: 0px; padding-bottom: 0px;">
								{% for collection in header_data.dropDownLink.collections %}
								<li class="level1 "><a href="{{ collection.itemlink.value }}">
										<span>{{ collection.itemname.value }}</span>
									</a>
								</li>
								{% endfor %}
							</ul>
						</li>
						{% endif %}

						{% if header_data.dropDownLink2.section_state == 'enabled' %}
						<li class="level0 nav-2 top-level parent">
							<a
								href="{{ header_data.dropDownLink2.fields.itemlink.value }}"><span>{{ header_data.dropDownLink2.fields.itemname.value }}</span></a>
							<ul class="level0 mobile-menu mobile-dropdown"
								style="height: 0px; padding-top: 0px; padding-bottom: 0px;">
								{% for collection in header_data.dropDownLink2.collections %}
								<li class="level1 "><a href="{{ collection.itemlink.value }}">
										<span>{{ collection.itemname.value }}</span>
									</a>
								</li>
								{% endfor %}
							</ul>
						</li>
						{% endif %}
						{% if header_data.boxMenu.section_state == 'enabled' %}
						{% for collection in header_data.boxMenu.collections %}
						<li class="level0 nav-2 top-level parent">
							<a href="javascript:void(0)"><span>{{ collection.linkName.value }}</span></a>
							<div class="level0 dropdown mobile-menu mobile-dropdown">
								<a class="dropdown-back desktop-hide" href="{{ link('common/home') }}"><i
										class="fi fi-left"></i>
									Back to Main Menu</a>
								<div class="dropdown-inner">
									{% set categories1 =  getCategoryAndChilds(collection['categories1']['value'], 500, 500) %}
									<div class="col-2">
										<p class="title mtitle">{{ categories1[loop.index -1].name }}</p>
										<div class="dropdown-links jm-dropdown-links">
											{% for sub_categories in categories1[loop.index -1]["sub_categories"]|slice(0, 7) %}
											<a href="{{ sub_categories.href }}">{{ sub_categories.name }}</a>
											{% endfor %}
											<a class="view-all-link" href="{{ categories1[loop.index -1].href }}">
												{{ collection.moreLink.value }}</a>
										</div>
									</div>
									{% set categories2 =  getCategoryAndChilds(collection['categories2']['value'], 500, 500) %}
									<div class="col-2">
										<p class="title mtitle">{{ categories2[loop.index -1].name }}</p>
										<div class="dropdown-links jm-dropdown-links">
											{% for sub_categories in categories2[loop.index -1]["sub_categories"]|slice(0, 7) %}
											<a href="{{ sub_categories.href }}">{{ sub_categories.name }}</a>
											{% endfor %}
											<a class="view-all-link" href="{{ categories2[loop.index -1].href }}">
												{{ collection.moreLink.value }}</a>
										</div>
									</div>
									{% set categories3 =  getCategoryAndChilds(collection['categories3']['value'], 500, 500) %}
									<div class="col-2">
										<p class="title mtitle">{{ categories3[loop.index -1].name }}</p>
										<div class="dropdown-links jm-dropdown-links">
											{% for sub_categories in categories3[loop.index -1]["sub_categories"]|slice(0, 7) %}
											<a href="{{ sub_categories.href }}">{{ sub_categories.name }}</a>
											{% endfor %}
											<a class="view-all-link" href="{{ categories3[loop.index -1].href }}">
												{{ collection.moreLink.value }}</a>
										</div>
									</div>
									{% if collection.adOption1.value == 1 %}
									<a class="menu-banner mobile-hide" href="{{ collection.adOptionLink1.value }}">
										<img alt="" class="lazyload" src="{{ image(collection.adOptionImg1.value) }}">
										<span class="menu-banner-text"
											style="color:#000000;font-size:2em;">{{ collection.adOptionTitle.value }}</span>
										<span class="menu-banner-shop">{{ collection.adOptionText.value }}</span>
									</a>
									{% endif %}
								</div>
							</div>
						</li>
						{% endfor %}
						{% endif %}
					</ul>
				</div>
			</nav>

			<span data-action="toggle-nav" class="nav-toggle-menu"><span>{{ lang('text_menu') }}</span><i
					aria-hidden="true" class="fa fa-bars"></i></span>

