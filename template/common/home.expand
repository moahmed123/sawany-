			<button type="button" id="cart-widget-close" class="cart-widget-close"> <span class="icon"></span> </button>
			<div class="minicart-title" id="minicart-title">
				<span class="user-free-text">Cart
				</span>
			</div>
		</header>
		<section class="cart-content">
			<div>
				<!---->
			</div>
			<!---->
			<div class="ajax-cart">
				{{ cart_content }}
			</div>
			<!---->
			<!---->
			<div>
				<!---->
			</div>
		</section>
		<!---->
	</div>
	<!---->

	<!---------------cart side menu--------------------->
	<div class="stick-to-top">
		<div class="box-header-nav">
			{% if header_data.stickyMenuLogo.section_state == 'enabled' %}
			<!-- logo -->
			<strong class="logo">
				<a href="{{ link('common/home') }}"><img src="{{ image(header_data.stickyMenuLogo.fields.logo.value) }}"
						title="{{ document_title }}" alt="{{ document_title }}"></a>
			</strong>
			<!-- logo -->
			{% endif %}
			{% if categories|length > 0 and header_data.sliderCategories.section_state == 'enabled' %}
			<span data-action="toggle-nav-cat" class="nav-toggle-menu nav-toggle-cat">
				<span>{{ lang('text_categories') }}</span><i aria-hidden="true" class="fa fa-bars"></i></span>
			<div class="block-nav-categori">
