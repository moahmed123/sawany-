							class="{{item_category["sub_categories"]|length > 0 ? 'parent' : '' }} {{ loop.index > 7 ? 'cat-link-orther' : '' }}">
							<a href="{{ item_category.href }}">
								{{ item_category.name }}
							</a>
							{% if item_category["sub_categories"]|length > 0 %}
							<span class="toggle-submenu"></span>
							<div class="submenu">
								<ul class="categori-list">
									<li class="col-sm-3">
										<ul>
											{% for sub_categories in item_category["sub_categories"] %}
											<li><a href="{{ sub_categories.href }}"
													title="{{sub_categories.name}}">{{ sub_categories.name }}</a></li>
											{% endfor %}
										</ul>
									</li>
								</ul>
							</div>
							{% endif %}
						</li>
						{% endfor %}
					</ul>

					{% if featured_categories|length > 7 %}
					<div class="view-all-categori">
						<a class="open-cate btn-view-all">{{ lang('text_all_categories') }}</a>
					</div>
					{% endif %}
				</div>
			</div>
			{% endif %}
			<nav class="nav-container mobile-menu search-fade-out" data-cs="mainNav">
				<div class="nav-container-inner">
					<ul id="nav" class="mobile-menu to-mobile-menu" data-swipe-position=".mobile-topmenu">
						{% if header_data.mainMenuLink.section_state == 'enabled' %}
