	<div class="modal c_popup-container login-popup loginregister-popup" id="login_modal" tabindex="-1" role="dialog"
		aria-labelledby="exampleModalLabel" aria-hidden="true">
		<div class="c_popup">
			<div class="c_close fi fi-cross">

			</div>
			<div class="c_popup_inner">
				<div class="account-login clearfix">
					<div class="page-title">
						<h1>{{ lang('button_login') }}</h1>
						<!-- <div class="subscribe-login"> 
				   <h2 class="popup-title">We noticed you have an account</h2> 
				   <p class="popup-subtitle">Login to receive a notification when this product arrives</p> 
			   </div>  -->
					</div>
					{% if d_social_login_enabled %}
					<div class="socialconnect-lg">
						<h2 class="title">
							<span class="login-part">{{ lang('with_social') }}</span>
						</h2>
						<div class="socialconnect-login">
							{{ d_social_login }}
						</div>
						<h3>{{ lang('with_mail') }}</h3>
					</div>
					{% endif %}
					{% if success %}
					<br>
					<div class="alert alert-success alert-dismissible" role="alert">
						<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
								aria-hidden="true">&times;</span></button>
						{{ success }}
					</div>
					{% endif %}

					{% if error_warning %}
					<br>
					<div class="alert alert-warning alert-dismissible" role="alert">
						<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
								aria-hidden="true">&times;</span></button>
						{{ error_warning }}
					</div>
					{% endif %}

					<form class="form-horizontal required-entry" action="{{ link('account/login', '', 'SSL') }}"
						method="post" enctype="multipart/form-data" id="login" novalidate="novalidate">
						<p class="error-message no-display"></p>
						<div class="form-group">
							<label class="input-box">
								<input type="text" id="loginInputName" value="{{ email }}" name="email"
									class="required-entry input-text validate-email validate-length maximum-length-255"
									autocomplete="email">
								<span class="label visible">{{ lang('entry_email') }}</span>
							</label> </div>
						<div class="form-group">
							<label class="input-box">
								<input type="password" name="password"
									class="validate-password required-entry input-text validate-password"
									id="loginInputEmail" placeholder="{{ lang('entry_password') }}"
									value="{{ password }}">
								<span class="label visible">{{ lang('entry_password') }}</span></label>
						</div>
						<div class="buttons-set">
							<button type="submit" class="btn btn-lg btn-block">
								<span><span>{{ lang('button_login') }}</span>
								</span>
							</button>
							{% if redirect %}
							<input type="hidden" name="redirect" value="{{ redirect }}" />
							{% endif %}
						</div>
					</form>
					<div class="buttons-set">
						<a href="{{ link('account/register', '', 'SSL') }}"
							class="btn btn-white btn-lg btn-block register-link loginregister-link">
							<span>{{ lang('Account_Register') }}</span>
						</a>
					</div>
				</div>


			</div>
		</div>
	</div>
	<div class="swipe-menu-wrapper no-display" data-cs="swipeMenu">
		<div class="swipe-menu">
			<div class="welcome-container">
				<div class="login-mobile-menu">
					<div>
						<span class="wellcome">
							<a href="{{ link('account/login') }}">{{ lang('Account_Login') }}</a> |
							<a href="{{ link('account/register') }}">{{ lang('Account_Register') }}</a>
						</span>
					</div>
				</div>
			</div>
			<span class="mtitle mobile-topmenu-mtitle desktop-hide"></span>
			<div class="mobile-topmenu desktop-hide"></div>
		</div>
	</div>
	<form id="contact-form" action="{{ link('information/contact') }}" method="POST" enctype="multipart/form-data">
		<input type="hidden" name="no-validate" value="1" />
		<input type="hidden" name="name" value="{{ (customer.getFirstname() ~ ' ' ~ customer.getLastname())|e }}" />
