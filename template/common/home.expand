		<input type="hidden" name="enquiry" />
		<input type="hidden" name="captcha" />
	</form>

	<script type="text/javascript">
		function contact_us(id) {
			$('#contact-form input[name="enquiry"]').val($('#enquiry-' + id).html() + '\n');
			$('#contact-form').submit();
		}
	</script>

